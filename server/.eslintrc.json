{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": 0,
        "max-len": "off",
        "no-unused-vars": "off",
        "new-cap": 0
    }
}
